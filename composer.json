{
    "name": "sakanjo/laravel-readable-migrations",
    "description": "Change migrations filenames to more readable numeric format.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "migrations",
        "laravel-migrations"
    ],
    "authors": [
        {
            "name": "Salah Kanjo",
            "email": "dev.salah.kanjo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0 || ^11.0",
        "illuminate/console": "^10.0 || ^11.0",
        "symfony/finder": "^7.0",
        "symfony/console": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.3",
        "phpstan/phpstan": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "SaKanjo\\ReadableMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SaKanjo\\ReadableMigrations\\Tests\\": "tests/src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SaKanjo\\ReadableMigrations\\ReadableMigrationsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "@php vendor/bin/pint --test",
            "@php vendor/bin/phpstan",
            "@php vendor/bin/pest --parallel"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
